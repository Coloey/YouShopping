/* pages/search/index.wxss */
page {
  background-color: #f5f5f5;
}
.search_row {
  display: flex;
}
.search_row input {
  flex: 9;
  margin: 0 20rpx;
  height: 70rpx;
  border-radius: 40rpx;
  background-color: #fff;
  border: 1rpx solid #dedede;
}
.search_row button {
  font-size: 30rpx;
  background-color: #fff;
  border: 1rpx solid #dedede;
  border-radius: 15rpx;
  flex: 1;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_content {
  padding: 20rpx;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 30rpx;
  padding: 15rpx;
  margin: 3rpx;
  padding: 15rpx 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
