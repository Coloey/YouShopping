<!--components/Tabs/Tabs.js.wxml-->
<view class="tabs">
    <view class="tabs_title">
        <view
        wx:for="{{tabs}}"
        wx:key="id"
        class="title_item {{item.isActive?'active':''}}"
        bindtap="changeIndex"
        data-index="{{index}}">
        {{item.value}}
        </view>
    </view>
    <view class="tabs_content"></view>
</view>
