/* components/UpImg/UpImg.wxss */
.up_img_wrap {
  display: flex;
}
.up_img_wrap .up_img {
  flex: 1;
  width: 90rpx;
  height: 90rpx;
  position: relative;
  margin: 0 10rpx;
}
.up_img_wrap .up_img image {
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
.up_img_wrap .up_img .iconfont {
  height: 60rpx;
  width: 60rpx;
  position: absolute;
  z-index: 2;
  top: -22rpx;
  right: -30rpx;
}
.up_img_wrap .up_img .iconfont icon {
  position: relative;
  z-index: 0;
}
